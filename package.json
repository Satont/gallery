{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:web": "webpack --config webpack.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run migration:up",
    "dev": "npm run prestart; nest start --watch",
    "dev:web": "webpack-dev-server --config webpack.ts --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.2.3",
    "@mikro-orm/core": "^4.2.3",
    "@mikro-orm/postgresql": "^4.2.3",
    "@mikro-orm/reflection": "^4.2.3",
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/platform-fastify": "^7.4.4",
    "axios": "^0.21.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "fastify": "^3.7.0",
    "fastify-static": "^3.3.0",
    "hbs": "^4.1.1",
    "point-of-view": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.0.2",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/express": "^4.17.8",
    "@types/hbs": "^4.0.1",
    "@types/jest": "^26.0.15",
    "@types/mini-css-extract-plugin": "^1.2.1",
    "@types/node": "^14.14.6",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.10",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "bootstrap": "^4.5.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.4",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "html-loader": "^1.3.2",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.6.1",
    "mini-css-extract-plugin": "^0.11.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
